<table class="table table-striped table table-bordered mt-5">
    <thead>
        <tr>
            <th scope="col">
                Id
            </th>
            <th scope="col">
                Titulo
            </th>
            <th scope="col">
                Contenido
            </th>
            <th scope="col">
                Creado
            </th>
            <th scope="col">
                Actualizado
            </th>
            <th scope="col">
                Editar/Eliminar
            </th>
        </tr>
    </thead>
    <tbody>
        <% noticias.forEach(noticia =>{ %>
            <tr class="">
                <th scope="row"><%= noticia.id %></th>
                <td><%= noticia.titulo %></td>
                <td><%= noticia.contenido %></td>
                <td><%= noticia.create_at %></td>
                <td class="text-center"><%= noticia.update_at ? noticia.update_at : "Sin datos"%></td>
                <td class="text-center"><a href="/noticias/editar/<%= noticia.id %>"><i class="fs-4 bi bi-pencil-fill"></i></a><a href="/noticias/eliminar/<%= noticia.id %>"><i class="fs-4 bi bi-trash3"></i></a></td>
            </tr>
        <% }); %>
    </tbody>
</table>